{% extends "base.html" %}

{% block content %}

<div class="post-inner">
  <h3>前20条微博：</h3><br />
    {% for weibo in weibolist %}
     <div class="box">
        <div class="inner">
          {{ forloop.counter }}.{{ weibo.text }}
          <p>{{ weibo.created }}  <a href="{% url comments weibo.id %}">评论</a></p>
        </div>
    </div>
    <br />
    {% endfor %}
</div>
{% endblock %}